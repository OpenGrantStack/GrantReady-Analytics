{
  "name": "grantready-analytics",
  "version": "1.0.0",
  "description": "Mobile-friendly analytics dashboards for grant progress, compliance, and outcomes",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "db:migrate": "node dist/database/migrations.js",
    "db:seed": "node dist/database/seed.js",
    "generate-docs": "typedoc --out docs/api src/index.ts",
    "prepare": "husky install",
    "docker:build": "docker build -t grantready-analytics .",
    "docker:run": "docker-compose up -d"
  },
  "keywords": [
    "analytics",
    "grants",
    "compliance",
    "dashboard",
    "government",
    "nonprofit",
    "metrics",
    "reporting"
  ],
  "author": "GrantReady Analytics Team",
  "license": "Apache-2.0",
  "dependencies": {
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "pg": "^8.11.3",
    "typeorm": "^0.3.17",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "winston": "^3.10.0",
    "moment": "^2.29.4",
    "axios": "^1.5.0",
    "zod": "^3.22.2",
    "chart.js": "^4.4.0",
    "csv": "^6.3.8",
    "pdfkit": "^0.14.0"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "ts-node-dev": "^2.0.0",
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.0",
    "@types/jest": "^29.5.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/cors": "^2.8.15",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "eslint": "^8.51.0",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "prettier": "^3.0.3",
    "typedoc": "^0.25.2",
    "husky": "^8.0.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/grantready-analytics.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/grantready-analytics/issues"
  },
  "homepage": "https://github.com/your-org/grantready-analytics#readme"
}
